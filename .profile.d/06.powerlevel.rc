# [POWERLEVEL9k]
# FOR COLORS LIST
alias term_colors='for code ({000..255}) print -P -- "$code: %F{$code}This is how your text would look like%f"'
# ALSO TO CHANGE COLORS
# POWERLEVEL9K_<name-of-segment>_<state>_[BACKGROUND|FOREGROUND]
# Segment		|	States
# -----------------------------------------------------------
# battery		|	LOW, CHARGING, CHARGED, DISCONNECTED
# context		|	DEFAULT, ROOT, SUDO, REMOTE, REMOTE_SUDO
# dir			|	HOME, HOME_SUBFOLDER, DEFAULT, ETC
# dir_writable		|	FORBIDDEN
# disk_usage		|	NORMAL, WARNING, CRITICAL
# host			|	LOCAL, REMOTE
# load			|	CRITICAL, WARNING, NORMAL
# rspec_stats		|	STATS_GOOD, STATS_AVG, STATS_BAD
# status		|	ERROR, OK (note: only, if verbose is not false)
# symfony2_tests	|	TESTS_GOOD, TESTS_AVG, TESTS_BAD
# user			|	DEFAULT, SUDO, ROOT
# vcs			|	CLEAN, UNTRACKED, MODIFIED
# vi_mode		|	NORMAL, INSERT
source /usr/share/zsh-theme-powerlevel9k/powerlevel9k.zsh-theme

POWERLEVEL9K_MODE="nerdfont-complete"

# Prompt
POWERLEVEL9K_DISABLE_RPROMPT=false
POWERLEVEL9K_PROMPT_ON_NEWLINE=true
POWERLEVEL9K_MULTILINE_LAST_PROMPT_PREFIX="‚èµ "
POWERLEVEL9K_MULTILINE_FIRST_PROMPT_PREFIX=""

POWERLEVEL9K_LEFT_PROMPT_ELEMENTS=(custom_elementary_icon dir dir_writable vcs virtualenv anaconda pyenv docker_machine aws ssh)
POWERLEVEL9K_RIGHT_PROMPT_ELEMENTS=(command_execution_time background_jobs)
POWERLEVEL9K_RPROMPT_ON_NEWLINE=true

# Segment Colors
POWERLEVEL9K_STATUS_OK_BACKGROUND='239'
POWERLEVEL9K_STATUS_OK_FOREGROUND='118'

POWERLEVEL9K_STATUS_ERROR_BACKGROUND='196'
POWERLEVEL9K_STATUS_ERROR_FOREGROUND='226'

POWERLEVEL9K_VCS_CLEAN_FOREGROUND='grey30'
POWERLEVEL9K_VCS_CLEAN_BACKGROUND='010'
POWERLEVEL9K_VCS_UNTRACKED_FOREGROUND='15'
POWERLEVEL9K_VCS_UNTRACKED_BACKGROUND='0'
POWERLEVEL9K_VCS_MODIFIED_FOREGROUND='grey30'
POWERLEVEL9K_VCS_MODIFIED_BACKGROUND='11'

POWERLEVEL9K_VIRTUALENV_BACKGROUND='13'
POWERLEVEL9K_VIRTUALENV_FOREGROUND='0'

POWERLEVEL9K_COMMAND_EXECUTION_TIME_BACKGROUND='13'
POWERLEVEL9K_COMMAND_EXECUTION_TIME_FOREGROUND='0'

POWERLEVEL9K_VI_MODE_NORMAL_BACKGROUND='seagreen1'
POWERLEVEL9K_VI_MODE_NORMAL_FOREGROUND='grey30'
POWERLEVEL9K_VI_MODE_INSERT_BACKGROUND='seagreen1'
POWERLEVEL9K_VI_MODE_INSERT_FOREGROUND='grey30'

POWERLEVEL9K_CUSTOM_ELEMENTARY_ICON="echo `whoami` "
POWERLEVEL9K_CUSTOM_ELEMENTARY_ICON_BACKGROUND='lightcoral'
POWERLEVEL9K_CUSTOM_ELEMENTARY_ICON_FOREGROUND='grey30'

POWERLEVEL9K_VCS_GIT_GITHUB_ICON=$'\uF408 '

ZSH_DISABLE_COMPFIX=true
